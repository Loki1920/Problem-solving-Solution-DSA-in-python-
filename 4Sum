class Solution:
    def fourSum(self, nums: List[int], target: int) -> List[List[int]]:
        com = itertools.combinations(nums,4)
        res = []

        for i in com:
            i = list(i)
            i = sorted(i)
            if sum(i) == target:
                if i not in res:
                    res.append(i)
        return res

        
