class Solution:
    def trailingZeroes(self, n: int) -> int:
        r = 1
        for i in range(1,n+1):
            r = r * i
        c = 0
        while r > 0:
            if r % 10 ==0:
                c+=1
            else:
                break
            r = r//10
        return c
            

        
